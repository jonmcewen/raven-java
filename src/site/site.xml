<project xmlns="http://maven.apache.org/DECORATION/1.3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
         name="Raven-Java">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <publishDate position="left"/>
    <custom>
        <fluidoSkin>
            <profile>pre-release</profile>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <gitHub>
                <projectId>${github.repo}</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>red</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

    <body>
        <links>
            <item name="Sentry" href="https://www.getsentry.com/"/>
            <item name="Sentry on GitHub" href="https://www.github.com/getsentry/sentry/"/>
        </links>
        <menu ref="parent" inherit="top"/>
        <menu ref="modules" inherit="top"/>
        <menu ref="reports" inherit="top"/>
    </body>
</project>
